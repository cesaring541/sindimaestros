<!DOCTYPE html>
<html>
<head>
  <title>Afiliados | Sindimaestros</title>
  
  <% include ./partials/head.ejs %>
</head>
<body class="hold-transition skin-blue sidebar-mini">
<!-- Site wrapper -->
<div class="wrapper">
  <% include ./partials/nav-header.ejs %>

  <% include ./partials/left-column.ejs %>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>Gestionar Afiliados</h1>
    </section>

    <section class="content">
      <div class="row">
        <div class="col-md-3 col-sm-4 col-xs-5">
          <div class="contains">
            <span class="btn bg-green" id="add-joined"> <i class="fa fa-plus-circle fa-5x pull-left" href="#"></i> Agregar Afiliado </span>
          </div>
        </div>
        <div class="col-md-3 col-sm-4 col-xs-5">
          <div class="contains">
            <span class="list-joineds btn btn-primary"><i class="fa fa-reorder fa-5x pull-left"></i> Listar Afiliados </span>
          </div>
        </div>
      </div>

      <!-- Alertas -->
      <% if (message.length > 0) { %>
        <div class="alert alert-danger" style="margin-top: 10px;"><%= message %></div>
      <% } %>

      <div class="box-body">

        <!-- Tabla de usuarios -->
        <div class="table-users fresh-table">
          <div class="toolbar">
            <!--button id="alertBtn" class="btn btn-default">Botón de Alerta</button-->
          </div>

          <table id="fresh-table" class="table">
            <thead>
              <th data-field="identityCard">Documento de identidad </th>
              <th data-field="fullname">Nombre </th>
              <th data-field="email">Email </th>
              <th data-field="state">Estado </th>
              <th data-field="actions" data-formatter="operateFormatter" data-events="operateEvents">Acciones </th>
            </thead>
            <tbody>
              <% if (objJoined != undefined) { %>
              <% objJoined.forEach(function(objJoi) { %>
              <tr class="<%= objJoi._id %>" style="text-align:center;">
                <td><%= objJoi.identityCard %></td>
                <td><%= objJoi.fullName %></td>
                <td><%= objJoi.email %></td>
                <% if (objJoi.state === "Activo") { %>
                  <td><i class="fa fa-circle text-success"></i>  <%= objJoi.state %></td>
                <% } else { %>
                  <td><i class="fa fa-circle text-danger"></i>  <%= objJoi.state %></td>
                <% } %>
              </tr>
              <% }); %>
              <% } else { %>
              <td colspan="" rowspan="" headers="">Para crear un usuario, dé click en el boton <span class="fa fa-plus-circle"></span></td>
              <% } %>
            </tbody>
          </table>
        </div>

